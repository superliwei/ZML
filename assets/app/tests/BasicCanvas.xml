<?xml version="1.0" encoding="utf-8"?>
<BasicCanvas position="static" class="navContent">
	<label text="BasicCanvas" position="static" class="header"></label>
	<BasicCanvas position="relative" height="200" margin="15px" background="#222222">
		<BasicCanvas id="mybox" x="10" y="10" width="100" height="100" border="solid 1px #ffffff" background="#000000" />
	</BasicCanvas>
	<BasicCanvas position="static" class="btContainer">
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="x:20" />
			<Click>
				<CanvasEvent id="mybox" action="set" x="20" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="y:20" />
			<Click>
				<CanvasEvent id="mybox" action="set" y="20" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="width:50" />
			<Click>
				<CanvasEvent id="mybox" action="set" width="50" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="height:50" />
			<Click>
				<CanvasEvent id="mybox" action="set" height="50" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="scaleX:0.5" />
			<Click>
				<CanvasEvent id="mybox" action="set" scaleX="0.5" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="scaleY:0.5" />
			<Click>
				<CanvasEvent id="mybox" action="set" scaleY="0.5" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="rotate:45" />
			<Click>
				<CanvasEvent id="mybox" action="set" rotate="45" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="alpha:0.5" />
			<Click>
				<CanvasEvent id="mybox" action="set" alpha="0.5" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="visible:false" />
			<Click>
				<CanvasEvent id="mybox" action="set" visible="false" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="border:solid 5px #ff0000" />
			<Click>
				<CanvasEvent id="mybox" action="set" border="solid 5px #ff0000" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="background:#ffff00" />
			<Click>
				<CanvasEvent id="mybox" action="set" background="#ffff00" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="border-radius:10px" />
			<Click>
				<CanvasEvent id="mybox" action="set" border-radius="10px" />
			</Click>
		</Button>
		<BasicCanvas position="static" clear="both"></BasicCanvas>
		<Button effect="alpha" position="static" class="btn" background="#3333ff">
			<Label position="static" text="reset" color="#ffffff" />
			<Click>
				<CanvasEvent id="mybox" action="set" x="10" y="10" width="100" height="100" border="solid 1px #ffffff" background="#000000" border-radius="0px" alpha="1" scaleX="1" scaleY="1" rotate="0" visible="true" />
			</Click>
		</Button>
		<BasicCanvas position="static" clear="both"></BasicCanvas>
	</BasicCanvas>
</BasicCanvas>