<?xml version="1.0" encoding="utf-8"?>
<BasicCanvas>
	<Label x="10" y="10" text="eval" color="#ffffff" background="#333333" padding="3" font-weight="bold"/>
	<BasicCanvas x="10" y="40" background="#222222" width="780" height="460">
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="10" padding="3px" text="Math.random" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					info.view.text(Math.random()*10);
				</eval>
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="40" padding="3px" text="$canvas(&quot;#canvasId&quot;)" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					var canvas = $canvas("#mainNav");
					var str = canvas.data.prop("outerHTML");
					info.view.text(str);
				</eval>
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="70" padding="3px" text="$canvas(&quot;Label&quot;)" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					var arr = $canvas("Label");
					var str="";
					$.each(arr,function(i){
						str+=arr[i].data.prop("outerHTML");
					})
					info.view.text(str);
				</eval>
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="100" padding="3px" text="$canvas(&quot;Label,background:#333333&quot;)" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					var arr = $canvas("Label,background:#333333");
					var str="";
					$.each(arr,function(i){
						str+=arr[i].data.prop("outerHTML");
					})
					info.view.text(str);
				</eval>
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="130" padding="3px" text="$canvas(&quot;Label,background:#333333,text:eval&quot;)" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					var arr = $canvas("Label,background:#333333,text:eval");
					var str="";
					$.each(arr,function(i){
						str+=arr[i].data.prop("outerHTML");
					})
					info.view.text(str);
				</eval>
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true">
			<Label x="10" y="160" padding="3px" text="$canvas(&quot;Label,text:Math.random&quot;)" border-radius="3px" background="#ffffff" />
			<Click>
				<eval>
					var info = $canvas("#evalInfo");
					var arr = $canvas("Label,text:Math.random");
					var str="";
					$.each(arr,function(i){
						str+=arr[i].data.prop("outerHTML");
					})
					info.view.text(str);
				</eval>
			</Click>
		</Button>
		<BasicCanvas x="10" y="250" width="760" height="200" border="solid 1px #666666" overflow="auto">
			<HtmlCanvas id="evalInfo" color="#ffffff" padding="5px"/>
		</BasicCanvas>
	</BasicCanvas>
</BasicCanvas>