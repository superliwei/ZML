<?xml version="1.0" encoding="utf-8"?>
<BasicCanvas position="static" class="navContent">
	<label text="Tween" position="static" class="header"></label>
	<BasicCanvas border="solid 1px #333333" height="350" position="relative" margin="15px" background="rgba(0,0,0,0.2)">
		<BasicCanvas id="box" x="60" y="60">
			<BasicCanvas id="box_canvas" width="100" height="100" border="solid 1px #ffffff" background="#111111" x="-50" y="-50" />
		</BasicCanvas>
	</BasicCanvas>
	<BasicCanvas position="static" class="btContainer">
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="x:175,y:75" />
			<Click>
				<Tween target="box" duration="0.5" x="175" y="75" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" x="175" y="75" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="width:150,height:150" />
			<Click>
				<Tween target="box_canvas" duration="0.5" width="150" height="150" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box_canvas" duration="0.5" width="150" height="150" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="scaleX:1.5,scaleY:1.5" />
			<Click>
				<Tween target="box" duration="0.5" scaleX="1.5" scaleY="1.5" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" scaleX="1.5" scaleY="1.5" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="scale:1.5,rotation:45" />
			<Click>
				<Tween target="box" duration="0.5" scale="1.5" rotation="45" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" scale="1.5" rotation="45" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="rotationX:45,rotationY:45" />
			<Click>
				<Tween target="box" duration="0.5" rotationX="45" rotationY="45" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" rotationX="45" rotationY="45" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="z:-1000" />
			<Click>
				<Tween target="box" duration="0.5" z="-1000" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" z="-1000" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="alpha:0.3" />
			<Click>
				<Tween target="box" duration="0.5" alpha="0.3" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" alpha="0.3" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="autoAlpha:0" />
			<Click>
				<Tween target="box" duration="0.5" autoAlpha="0" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" autoAlpha="0" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="autoAlpha:1" />
			<Click>
				<Tween target="box" duration="0.5" autoAlpha="1" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" autoAlpha="1" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="border:10px&nbsp;#ff0000" />
			<Click>
				<Tween target="box_canvas" duration="0.5" border="10px #ff0000" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box_canvas" duration="0.5" border="10px #ff0000" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="backgroundColor:#ffff00" />
			<Click>
				<Tween target="box_canvas" duration="0.5" backgroundColor="#ffff00" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box_canvas" duration="0.5" backgroundColor="#ffff00" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="border-radius:20px" />
			<Click>
				<Tween target="box_canvas" duration="0.5" border-radius="20px" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box_canvas" duration="0.5" border-radius="20px" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="action:from&nbsp;x:0" />
			<Click>
				<Tween action="from" target="box" duration="0.5" x="0" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween action="from" target="box" duration="0.5" x="0" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="action:fromTo&nbsp;x:10,275" />
			<Click>
				<Tween action="fromTo" target="box" duration="0.5" x="10,275" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween action="fromTo" target="box" duration="0.5" x="10,275" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="ease:Expo.easeInOut&nbsp;x:55" />
			<Click>
				<Tween target="box" duration="1" x="55" ease="Expo.easeInOut" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="1" x="55" ease="Expo.easeInOut" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,delay:1" />
			<Click>
				<Tween id="tween0" target="box" duration="1" x="55" delay="1" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" target="box" duration="1" x="55" delay="1" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:kill" />
			<Click>
				<Tween id="tween0" action="kill" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="kill" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:pause" />
			<Click>
				<Tween id="tween0" action="pause" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="pause" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:play" />
			<Click>
				<Tween id="tween0" action="play" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="play" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:restart" />
			<Click>
				<Tween id="tween0" action="restart" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="restart" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:resume" />
			<Click>
				<Tween id="tween0" action="resume" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="resume" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="id:tween0,action:reverse" />
			<Click>
				<Tween id="tween0" action="reverse" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween id="tween0" action="reverse" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="action:killTweensOf" />
			<Click>
				<Tween action="killTweensOf" target="box" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween action="killTweensOf" target="box" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="onStart,onUpdate,onComplete" />
			<Click>
				<Tween target="box" duration="0.5" x="100">
					<onStart>
						<CanvasEvent id="box_canvas" action="set" background="#ff0000" />
					</onStart>
					<onUpdate>
						<CanvasEvent id="box_canvas" action="set" background="#ffff00" />
					</onUpdate>
					<onComplete>
						<CanvasEvent id="box_canvas" action="set" background="#111111" />
					</onComplete>
				</Tween>
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Tween target="box" duration="0.5" x="100"&gt;<br/>
				&nbsp;&nbsp;	&lt;onStart&gt;<br/>
	&nbsp;&nbsp;&nbsp;&nbsp;		&lt;CanvasEvent id="box_canvas" action="set" background="#ff0000" /&gt;<br/>
				&nbsp;&nbsp;	&lt;/onStart&gt;<br/>
				&nbsp;&nbsp;	&lt;onUpdate&gt;<br/>
	&nbsp;&nbsp;&nbsp;&nbsp;		&lt;CanvasEvent id="box_canvas" action="set" background="#ffff00" /&gt;<br/>
				&nbsp;&nbsp;	&lt;/onUpdate&gt;<br/>
				&nbsp;&nbsp;	&lt;onComplete&gt;<br/>
	&nbsp;&nbsp;&nbsp;&nbsp;		&lt;CanvasEvent id="box_canvas" action="set" background="#111111" /&gt;<br/>
				&nbsp;&nbsp;	&lt;/onComplete&gt;<br/>
							&lt;/Tween&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="Delay&nbsp;time:1" />
			<Click>
				<Delay id="delay0" time="1">
					<CanvasEvent id="box_canvas" action="set" background="#ff0000" />
				</Delay>
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Delay id="delay0" time="1"&gt;<br/>
				&nbsp;&nbsp;	&lt;CanvasEvent id="box_canvas" action="set" background="#ff0000" /&gt;<br/>
							&lt;/Delay&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="Delay&nbsp;action:kill" />
			<Click>
				<Delay id="delay0" action="kill" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							&lt;Delay id="delay0" action="kill" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn" background="#6666ff">
			<Label position="static" text="reset" color="#ffffff" />
			<Click>
				<Tween action="killTweensOf" target="box" />
				<Tween action="killTweensOf" target="box_canvas" />
				<CanvasEvent id="box" action="set" x="60" y="60" scaleX="1" scaleY="1"/>
				<Tween target="box" duration="0" scale="1" rotation="0" rotationX="0" rotationY="0" z="0" autoAlpha="1" />
				<CanvasEvent id="box_canvas" action="set" width="100" height="100" border="solid 1px #ffffff" background="#111111" border-radius="0px" />
				<Delay id="delay0" action="kill" />
				<CanvasEvent id="info" action="set" html="content">
					<content>
						<div>
							基本用法:<br>
							&lt;Tween target="box" duration="0.5" x="10" /&gt;
						</div>
					</content>
				</CanvasEvent>
			</Click>
		</Button>
		<BasicCanvas position="static" clear="both"></BasicCanvas>
	</BasicCanvas>
	<HtmlCanvas id="info" color="#ffffff" padding="5px" margin="15px" position="static" background="rgba(0,0,0,0.5)" max-height="100px" overflow="auto">
		<div>
			基本用法:<br>
			&lt;Tween target="box" duration="0.5" x="10" /&gt;
		</div>
	</HtmlCanvas>
</BasicCanvas>