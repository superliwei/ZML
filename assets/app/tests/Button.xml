<?xml version="1.0" encoding="utf-8"?>
<BasicCanvas position="static" class="navContent">
	<label text="Button" position="static" class="header"></label>
	<BasicCanvas position="static" class="btContainer">
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="Button" />
			<Click>
				<Alert msg="Button Clicked !" />
			</Click>
		</Button>
		<Button effect="alpha" touchMode="true" position="static" class="btn">
			<Label position="static" text="touchMode" />
			<Click>
				<Alert msg="Button Clicked !" />
			</Click>
		</Button>
		<BasicCanvas position="static" clear="both"></BasicCanvas>
		<Button id="cbt" effect="alpha" position="static" class="btn">
			<Label position="static" text="mouseEnable Control" />
			<Click>
				<Alert msg="Button Clicked !" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="enable" />
			<Click>
				<CanvasEvent id="cbt" action="set" enabled="true" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="disable" />
			<Click>
				<CanvasEvent id="cbt" action="set" enabled="false" />
			</Click>
		</Button>
	</BasicCanvas>
</BasicCanvas>