<?xml version="1.0" encoding="utf-8"?>
<BasicCanvas position="static" class="navContent">
	<label text="ImageButton" position="static" class="header"></label>
	<BasicCanvas position="relative" margin="15px" height="60">
		<ImageButton id="imageBt" url="images/imageButton.png" preload="true" effect="alpha">
			<Click>
				<Alert msg="ImageButton Clicked !" />
			</Click>
		</ImageButton>
	</BasicCanvas>
	<BasicCanvas position="static" class="btContainer">
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="enable" />
			<Click>
				<CanvasEvent id="imageBt" action="set" enabled="true" />
			</Click>
		</Button>
		<Button effect="alpha" position="static" class="btn">
			<Label position="static" text="disable" />
			<Click>
				<CanvasEvent id="imageBt" action="set" enabled="false" />
			</Click>
		</Button>
	</BasicCanvas>
</BasicCanvas>